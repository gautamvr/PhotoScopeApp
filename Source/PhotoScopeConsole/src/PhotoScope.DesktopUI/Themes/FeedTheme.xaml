<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
   
    <DataTemplate x:Key="FeedItemTemplate">
        <Border Margin="2" >
            <Border BorderBrush="Blue" BorderThickness="2">
                <StackPanel>
                <!--<Rectangle Fill="Blue" Width="30" Height="30" DataContext="{Binding Path=ImageUri}"></Rectangle>-->
                <Image Stretch="None">
                    <Image.Source>
                        <BitmapImage UriCachePolicy="Revalidate" UriSource="{Binding Path=Url_s}"/>
                    </Image.Source>
                </Image>
                <TextBlock Text="{Binding Path=Title}"></TextBlock>
                </StackPanel>
            </Border>
        </Border>
    </DataTemplate>

</ResourceDictionary>